<?xml version="1.0" encoding="utf-8"?>
<s:Panel xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 xmlns:mx="library://ns.adobe.com/flex/mx"
		 title="Mes contacts">
	<fx:Declarations>
		<s:ArrayCollection id="fakeContacts">
			<fx:Object firstName="Mickey" lastName="Mouse"/>
			<fx:Object firstName="Donald" lastName="Duck"/>
		</s:ArrayCollection>
	</fx:Declarations>
	
	<s:List width="100%" height="100%" dataProvider="{fakeContacts}"
			itemRenderer="ttfx.bdx.tdd.contacts.view.ContactItemRenderer">
	</s:List>
</s:Panel>
